{
  "name": "styles",
  "version": "0.0.0",
  "main": "index.js",
  "engines": {
    "node": "0.12.x"
  },
  "scripts": {
    "clean": "./clean",
    "build:documents": "./publish",
    "build:styles": "./styles",
    "build": "npm run build:documents && npm run build:styles",
    "deploy": "git push origin gh-pages:gh-pages",
    "publish": "npm run build",
    "publish:master": "git merge --no-ff --no-edit master && npm run build && git add . && git commit -m \"build latest master.\" && git push origin gh-pages",
    "start": "node index.js",
    "start:debug": "node-debug index.js",
    "test": "./node_modules/.bin/tap test/*.js",
    "test:watch": "./watcher.js"
  },
  "dependencies": {
    "autoprefixer": "^6.0.0",
    "babel": "^5.0.0",
    "body-parser": "^1.13.1",
    "cookie-parser": "^1.3.4",
    "ejs": "^2.0.0",
    "express": "^4.0.0",
    "marked": "^0.3.0",
    "node-sass": "^2.0.0",
    "postcss": "^5.0.0"
  },
  "devDependencies": {
    "chokidar": "^1.4.3",
    "node-inspector": "^0.8.1",
    "postcss-cli": "^2.1.0",
    "tap": "^5.7.0"
  }
}
